datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String    @id @default(cuid())
  name     String?
  email    String    @unique
  image    String?
  students Student[]
}

model Student {
  id        String   @id @default(cuid())
  userId    String
  name      String
  email     String
  subject   String
  marks     Int
  examDate  DateTime

  user      User     @relation(fields: [userId], references: [id])
}
